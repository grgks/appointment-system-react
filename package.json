    "selenium-webdriver": "^4.36.0",
    "ui": "^0.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@jest/globals": "^30.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/selenium-webdriver": "^4.35.1",
    "@vitejs/plugin-react": "^4.6.0",
