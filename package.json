{
  "name": "appointment-system-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "jest tests/e2e && node tests/e2e/allure-environment.js",
    "test:e2e:watch": "jest tests/e2e --watch",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
