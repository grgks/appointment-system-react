    "jest": "^30.2.0",
    "jest-allure2-reporter": "^2.2.8",
    "jest-html-reporter": "^4.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "vite": "^7.0.4"
  }
